from turn_arr-to-path-test import *
import unittest

class TestTurnArrToPath(unittest.TestCase):

    def test_small_rect_path(self):
        I = [2,3,5]
        O = {(0,0,0):  1,  # 1
             (-1,0,0): 1,  # 2
             (-1,0,3): 1,  # 3
             (-1,0,2): 1,  # 4
             (0,0,2):  1,  # 5
             (0,0,1):  1}  # 6
        self.assertEqual(make_path(I),O)

if __name__ == "__main__":
    unittest.main()
